import re

def is_valid_format(input_string):
    # Define the regex pattern to match "5b" format
    pattern = r'^\d+b$'  # ^ : start of string, \d+ : one or more digits, b : literal 'b', $ : end of string
    
    # Use re.match to check if the input string matches the pattern
    match = re.match(pattern, input_string)
    
    # Return True if there is a match, False otherwise
    return match is not None

# Test the function
test_strings = ["5b", "10b", "123b", "b5", "5", "5c", "b5c"]
for test_string in test_strings:
    print(f"{test_string}: {is_valid_format(test_string)}")
