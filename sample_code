import pandas as pd
from pandas.tseries.offsets import BDay
from datetime import datetime

# Example date
date1 = datetime.now().date()

# Define the number of business days to go back
business_days = 5

# Create a DatetimeIndex with the range of dates
date_range = pd.date_range(end=date1, periods=business_days, freq=BDay())

# Convert DatetimeIndex to list of dates
business_dates = date_range.to_pydatetime().tolist()

print("Business dates:", business_dates)
